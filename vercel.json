{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "web/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/web/$1"
      }
    ],
    "functions": {
    "api/server.js": {
      "includeFiles": "api/prisma/schema.prisma"
    }
  }
  }
  